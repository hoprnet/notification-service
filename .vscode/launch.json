{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "notify-matrix",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "preLaunchTask": "npm: build",
            "program": "${workspaceFolder}/src/app.ts",
            "envFile": "${workspaceFolder}/.env",
            "console": "integratedTerminal",
            "outFiles": [
                "${workspaceFolder}/lib/*.js"
            ]
        }
    ]
}